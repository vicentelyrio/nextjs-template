  "repository": {
    "type": "git",
    "url": "git+https://github.com/vicentelyrio/nextjs-template.git"
  },
  "homepage": "https://github.com/vicentelyrio/nextjs-template#readme",
  "lint-staged": {
    "**/*.js": [
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
