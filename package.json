{
  "name": "nextjs-template",
  "private": true,
  "version": "1.0.0",
  "description": "NextJS Template",
  "author": "vicentelyrio@gmail.com",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
